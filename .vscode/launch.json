{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
        "name": "Attach to Chrome",
        "port": 9222,
        "request": "attach",
        "type": "chrome",
        "webRoot": "${workspaceFolder}\\music_controller"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}\\music_controller\\manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via npm",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\music_controller\\frontend",
      "runtimeArgs": ["run-script", "debug"]
    }
  ]
}
